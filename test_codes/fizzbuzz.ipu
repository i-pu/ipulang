fn judge(i: i32): unit {
    if (i % 3 == 0) {
        printstr("fizz\n");
        return;
    }
    if (i % 5 == 0) {
        printstr("buzz\n");
        return;
    }
    if (i % 15 == 0) {
        printstr("fizzbuzz\n");
        return;
    }
    printi32(i);
    printstr("\n");
    return;
}

fn main(): unit {
    for (var i: i32; i < 30; i = i + 1;) {
        judge(i);
    }
}